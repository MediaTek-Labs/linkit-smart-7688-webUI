 {
 "parser": "babel-eslint",
   "env": {
      "browser": true,
      "node": true
   },
   "globals": {
      "__": true,
      "STRING": true,
      "jest": true,
      "describe": true,
      "it": true,
      "xit": true
   },
   "rules": {
      "handle-callback-err": [0, "^(err|error|anySpecificError)$" ],
      "quotes": [1, "single"],
      "strict": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": 0,
      "no-multi-spaces": 0,
      "key-spacing": 0,
      "no-return-assign": 0,
      "consistent-return": 0,
      "no-shadow": 0,
      "no-comma-dangle": 0,
      "no-use-before-define": 0,
      "no-empty": 0,
      "new-cap": true,
      "dot-notation": 0,
      "new-parens": 0,
      "camelcase": false,
      "no-cond-assign": 0
   }
 }
